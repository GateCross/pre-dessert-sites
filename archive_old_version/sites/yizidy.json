{
  "id": "yizidy",
  "name": "椅子电影",
  "domain": "http://www.yizidy.com",
  "encoding": "UTF-8",
  "public": true,
  "search": {
    "paths": [
      {
        "path": "index.php?s=vod-search",
        "method": "post"
      }
    ],
    "data": {
      "typeid": 0,
      "wd": "{{keyword}}"
    },
    "rules": [
      {
        "selectors": [".movielist"],
        "allow": ["/vod/\\d+/"],
        "torrents": true
      }
    ]
  },
  "torrents": {
    "fields": null
  }
}
