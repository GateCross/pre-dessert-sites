{
  "id": "hares",
  "name": "白兔",
  "domain": "https://club.hares.top",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "inactive_day": 7,
    "has_free": true,
    "has_2x_free": true
  },
  "requests": {
    "torrents": {
      "list": {
        "selector": "table.torrents > tr"
      }
    },
    "user_basic_info": {
      "fields": {
        "uploaded": {
          "selector": "/html/body/div[1]/div/div/div/ul/li[4]/span",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "downloaded": {
          "selector": "/html/body/div[1]/div/div/div/ul/li[5]/span",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    },
    "details": {
      "fields": {
        "free": {
          "selector": "//b[@class='free'][text()='免费']",
          "filters": [
            {
              "name": "not_blank"
            }
          ]
        },
        "2x_free": {
          "selector": "//b[@class='twoupfree'][text()='2X免费']",
          "filters": [
            {
              "name": "not_blank"
            }
          ]
        }
      }
    }
  }
}
