{
  "id": "soulvoice",
  "name": "聆音",
  "domain": "https://pt.soulvoice.club",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/电视剧"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Show/综艺"
      }
    ]
  },
  "requests": {
    "details": {
      "fields": {
        "hr": {
          "selector": "//img[@class='hitandrun']",
          "filters": [
            {
              "name": "not_blank"
            }
          ]
        }
      }
    }
  }
}
