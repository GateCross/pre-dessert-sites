{
  "id": "carpt",
  "name": "小车站",
  "domain": "https://carpt.net/",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/连续剧"
      },
      {
        "id": 403,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV",
        "desc": "TV Shows/综艺"
      },
      {
        "id": 406,
        "cat": "Music",
        "desc": "Music/音乐"
      }
    ]
  },
  "requests":{
    "search": {
      "fields": {
        "hr_days":{
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "labels": {
          "selector": "td:nth-child(1) > span[title=\"\"]",
          "array": true
        },
        "description": {
          "selector": "td:nth-child(1)",
          "remove": "span.label,a,img,b,font,br",
          "contents": -1
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
}
