{
  "id": "audiences",
  "name": "观众",
  "domain": "https://audiences.me",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影/Movies"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "剧集/TV-Series"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "综艺/TV-Show"
      },
      {
        "id": 406,
        "cat": "TV/Documentary",
        "desc": "纪录片/Documentary"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "category": {
          "selector": "a[href*='?cat=']",
          "attribute": "href",
          "filters": [
            {
              "name": "replace",
              "args": [
                "?",
                ""
              ]
            },
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "imdbid": {
          "selector": "div.imdb_100 > a",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "tt\\d+",
                1
              ]
            }
          ]
        },
        "description": {
          "selector": "td.embedded > span[style]"
        },
        "labels": {
          "array": true,
          "selector": "td.embedded > span.tags"
        },
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "2",
                "0"
              ]
            }
          ]
        }
      }
    },
    "seeding_statistics": {
      "parser": "CssSelector",
      "disabled_extends": {
        "headers": true,
        "list": true
      },
      "fields": {
        "count": {
          "selector": "table.table.table-bordered tr:has(td:contains(Total)) td:nth-child(2)"
        },
        "size": {
          "selector": "table.table.table-bordered tr:has(td:contains(Total)) td:nth-child(3)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    }
  }
}
