{
  "id": "piggo",
  "name": "猪猪",
  "domain": "https://piggo.me",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV/Series",
        "desc": "电视剧"
      },
      {
        "id": 403,
        "cat": "TV/Shows",
        "desc": "综艺"
      },
      {
        "id": 404,
        "cat": "TV/Documentaries",
        "desc": "纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Animations",
        "desc": "动漫"
      },
      {
        "id": 908,
        "cat": "TV/Series",
        "desc": "儿童剧集"
      },
      {
        "id": 909,
        "cat": "Movies",
        "desc": "儿童电影"
      }
    ]
  },
  "requests":{
    "search": {
      "fields": {
        "hr_days": {
          "selector": "img.kaohe",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "3",
                "0"
              ]
            }
          ]
        },
        "date_elapsed": {
          "selector": "td.rowfollow:nth-child(4)",
          "remove": "br"
        },
        "date_added": {
          "selector": "td.rowfollow:nth-child(4)",
          "remove": "br",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
}
