{
  "id": "mteam",
  "name": "馒头",
  "domain": "https://kp.m-team.cc",
  "api": "https://api.m-team.cc",
  "public": false,
  "schema": "mTorrent",
  "reuse_schema": "NexusPHP",
  "required": {
    "user_id": true
  },
  "meta": {
    "inactive_day": 7,
    "has_free": true,
    "has_2x_free": true,
    "has_cover": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影/SD"
      },
      {
        "id": 419,
        "cat": "Movies",
        "desc": "电影/HD"
      },
      {
        "id": 420,
        "cat": "Movies",
        "desc": "电影/DVDiSo"
      },
      {
        "id": 421,
        "cat": "Movies",
        "desc": "电影/Blu-Ray"
      },
      {
        "id": 439,
        "cat": "Movies",
        "desc": "电影/Remux"
      }
    ],
    "tv": [
      {
        "id": 403,
        "cat": "TV",
        "desc": "剧集/SD"
      },
      {
        "id": 402,
        "cat": "TV",
        "desc": "剧集/HD"
      },
      {
        "id": 438,
        "cat": "TV",
        "desc": "剧集/BluRay"
      },
      {
        "id": 435,
        "cat": "TV",
        "desc": "剧集/DVDiSo"
      }
    ]
  },
  "requests": {
    "get_subtitle_url": {
      "disabled": true
    },
    "search": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/search",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "id": {},
        "category": {},
        "title": {
          "selector": "name"
        },
        "cover": {
          "selector": "imageList.[0]"
        },
        "details": {
          "selector": "id",
          "filters": [
            {
              "name": "append_left",
              "args": "/detail/"
            }
          ]
        },
        "size": {},
        "grabs": {
          "selector": "status.timesCompleted"
        },
        "seeders": {
          "selector": "status.seeders"
        },
        "leechers": {
          "selector": "status.leechers"
        },
        "date_added": {
          "selector": "createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "downloadvolumefactor": {
          "selector": "status.discount",
          "filters": [
            {
              "name": "case",
              "args": {
                "FREE": "0",
                "PERCENT_50": "0.5",
                "PERCENT_70": "0.3",
                "NORMAL": "1"
              }
            }
          ]
        },
        "uploadvolumefactor": {
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "description": {
          "selector": "smallDescr"
        },
        "labels": {
          "array": true,
          "filters": [
            {
              "name": "case",
              "args": {
                "0": "",
                "1": "DIY",
                "2": "国配",
                "4": "中字",
                "5": "DIY|中字",
                "6": "国配|中字",
                "7": "DIY|国配|中字"
              }
            }
          ]
        }
      }
    },
    "my_peer_status": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/tracker/myPeerStatus",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "fields": {
        "leecher": {
          "selector": "data.leecher"
        },
        "seeder": {
          "selector": "data.seeder"
        }
      }
    },
    "msg_notify_statistic": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/msg/notify/statistic",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "fields": {
        "count": {
          "selector": "data.count"
        },
        "un_make": {
          "selector": "data.unMake"
        }
      }
    },
    "profile": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/member/profile",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "uid": [
          "{userId}"
        ]
      },
      "fields": {
        "created_date": {
          "selector": "data.createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "last_modified_date": {
          "selector": "data.lastModifiedDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "username": {
          "selector": "data.username"
        },
        "uploaded": {
          "selector": "data.memberCount.uploaded"
        },
        "downloaded": {
          "selector": "data.memberCount.downloaded"
        },
        "share_rate": {
          "selector": "data.memberCount.shareRate"
        },
        "bonus": {
          "selector": "data.memberCount.bonus"
        },
        "role": {
          "selector": "data.role"
        }
      }
    },
    "user_torrent_list": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/member/getUserTorrentList",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "size": {
          "selector": "torrent.size"
        }
      }
    },
    "sys_role_list": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/member/sysRoleList",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data"
      },
      "fields": {
        "id": {},
        "name_chs": {
          "selector": "nameChs"
        },
        "name_eng": {
          "selector": "nameEng"
        },
        "image": {
          "filters": [
            {
              "name": "append_left",
              "args": "/static/roles/"
            }
          ]
        }
      }
    },
    "notice": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/news/list",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data"
      },
      "fields": {
        "title": {
          "selector": "subject"
        },
        "date": {
          "selector": "createdDate"
        },
        "content": {}
      }
    },
    "messages": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/msg/search",
      "use_api": true,
      "required_headers": [
        "x-api-key",
        "authorization"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "keyword": [
          ""
        ],
        "box": [
          "-2"
        ],
        "pageNumber": [
          "{pageNumber}"
        ],
        "pageSize": [
          "100"
        ]
      },
      "list": {
        "selector": "data.data.#(unread=true)#"
      },
      "fields": {
        "id": {},
        "head": {
          "selector": "title"
        },
        "date": {
          "selector": "createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "content": {},
        "unread": {}
      }
    },
    "mark_as_read": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/msg/markRead",
      "use_api": true,
      "required_headers": [
        "x-api-key",
        "authorization"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "msgIds": [
          "{ids}"
        ]
      },
      "fields": {
        "success": {
          "selector": "code",
          "filters": [
            {
              "name": "eq",
              "args": "0"
            }
          ]
        },
        "message": {}
      }
    },
    "gen_dl_token": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/genDlToken",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "id": [
          "{id}"
        ]
      },
      "fields": {
        "url": {
          "selector": "data"
        }
      }
    },
    "details": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/detail",
      "use_api": true,
      "required_headers": [
        "x-api-key",
        "authorization"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "id": [
          "{id}"
        ]
      },
      "fields": {
        "absent": {
          "selector": "message",
          "filters": [
            {
              "name": "eq",
              "args": "種子未找到"
            }
          ]
        },
        "free": {
          "selector": "status.discount",
          "filters": [
            {
              "name": "eq",
              "args": "FREE"
            }
          ]
        },
        "peer_count": {
          "selector": "status.seeders"
        }
      }
    }
  }
}
