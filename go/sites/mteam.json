{
  "id": "mteam",
  "name": "馒头",
  "domain": "https://kp.m-team.cc/",
  "api": "https://api.m-team.cc/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "mTorrent",
  "reuse_schema": "NexusPHP",
  "price": {
    "RENDER": true,
    "FREE": [
      "true"
    ],
    "2XFREE": [
      "true"
    ],
    "HR": [],
    "PEER_COUNT": [
      "true"
    ]
  },
  "requests": {
    "search": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/torrent/search",
      "use_api": true,
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "id": {
          "selector": "id"
        },
        "title": {
          "selector": "name"
        },
        "details": {
          "selector": "smallDescr"
        },
        "poster": {
          "selector": "imageList.0"
        },
        "size": {
          "selector": "size"
        },
        "seeders": {
          "selector": "status.seeders"
        },
        "leechers": {
          "selector": "status.leechers"
        },
        "date_added": {
          "selector": "createdDate"
        },
        "downloadvolumefactor": {
          "selector": "status.discount",
          "case": {
            "FREE": 0,
            "PERCENT_50": 0.5,
            "PERCENT_70": 0.3,
            "NORMAL": 1
          }
        },
        "uploadvolumefactor": {
          "case": {
            "": 1
          }
        },
        "grabs": {
          "selector": "status.timesCompleted"
        }
      }
    },
    "my_peer_status": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/tracker/myPeerStatus",
      "use_api": true,
      "fields": {
        "leecher": {
          "selector": "data.leecher"
        },
        "seeder": {
          "selector": "data.seeder"
        }
      }
    },
    "msg_notify_statistic": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/msg/notify/statistic",
      "use_api": true,
      "fields": {
        "count": {
          "selector": "data.count"
        },
        "un_make": {
          "selector": "data.unMake"
        }
      }
    },
    "profile": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/member/profile",
      "use_api": true,
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "uid": [
          "{userId}"
        ]
      },
      "fields": {
        "created_date": {
          "selector": "data.createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "last_modified_date": {
          "selector": "data.lastModifiedDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "username": {
          "selector": "data.username"
        },
        "uploaded": {
          "selector": "data.memberCount.uploaded"
        },
        "downloaded": {
          "selector": "data.memberCount.downloaded"
        },
        "share_rate": {
          "selector": "data.memberCount.shareRate"
        },
        "bonus": {
          "selector": "data.memberCount.bonus"
        },
        "role": {
          "selector": "data.role"
        }
      }
    },
    "user_torrent_list": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/member/getUserTorrentList",
      "use_api": true,
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "size": {
          "selector": "torrent.size"
        }
      }
    },
    "sys_role_list": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/member/sysRoleList",
      "use_api": true,
      "list": {
        "selector": "data"
      },
      "fields": {
        "id": {
          "selector": "id"
        },
        "name_chs": {
          "selector": "nameChs"
        },
        "name_eng": {
          "selector": "nameEng"
        }
      }
    },
    "latest_notice": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/news/list",
      "use_api": true,
      "fields": {
        "title": {
          "selector": "data.0.subject"
        },
        "date": {
          "selector": "data.0.createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "content": {
          "selector": "data.0.context"
        }
      }
    },
    "unread_messages": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "api/msg/search",
      "use_api": true,
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "keyword": [
          ""
        ],
        "box": [
          "-2"
        ],
        "pageNumber": [
          "{pageNumber}"
        ],
        "pageSize": [
          "100"
        ],
        "unread": [
          "true"
        ]
      },
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "head": {
          "selector": "title"
        },
        "date": {
          "selector": "createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "content": {
          "selector": "context"
        }
      }
    }
  }
}