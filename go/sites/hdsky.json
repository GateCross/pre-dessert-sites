{
  "id": "hdsky",
  "name": "天空",
  "domain": "https://hdsky.me",
  "public": false,
  "schema": "hdsky",
  "reuse_schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      },
      {
        "id": 410,
        "cat": "Movies",
