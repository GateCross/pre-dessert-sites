      },
      {
        "id": 414,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(分集)"
      },
      {
        "id": 415,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(合集)"
      }
    ]
  },
  "search_box": {
    "fields": [
      {
        "title": "媒介",
        "name": "medium",
        "type": "checkbox",
        "options": [
          {
            "text": "UHD Blu-ray",
            "value": "medium13"
          },
          {
            "text": "UHD Blu-ray/DIY",
            "value": "medium14"
          },
          {
            "text": "Blu-ray",
            "value": "medium1"
          },
          {
            "text": "Blu-ray/DIY",
            "value": "medium12"
          },
          {
            "text": "Remux",
            "value": "medium3"
          },
          {
            "text": "Encode",
            "value": "medium7"
          },
          {
            "text": "HDTV",
            "value": "medium5"
          },
          {
            "text": "DVDR",
            "value": "medium6"
          },
          {
            "text": "CD",
            "value": "medium8"
          },
          {
            "text": "MiniBD",
            "value": "medium4"
          },
          {
            "text": "Track",
            "value": "medium9"
          },
          {
            "text": "WEB-DL",
            "value": "medium11"
          },
          {
            "text": "SACD",
            "value": "medium15"
          },
          {
            "text": "HD DVD",
            "value": "medium2"
          },
          {
            "text": "3D Blu-ray",
            "value": "medium16"
          }
        ]
      },
      {
        "title": "分辨率",
        "name": "standard",
        "type": "checkbox",
        "options": [
          {
            "text": "4K/2160p",
            "value": "standard5"
          },
          {
            "text": "2K/1080p",
            "value": "standard1"
          },
          {
            "text": "1080i",
            "value": "standard2"
          },
          {
            "text": "720p",
            "value": "standard3"
          },
          {
            "text": "SD",
            "value": "standard4"
          },
          {
            "text": "8K/4320P",
            "value": "standard6"
          }
        ]
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "download": {
          "selector": "form[action*='/download.php?id=']",
          "attribute": "action"
        },
        "description": {
          "any": [
            {
              "selector": "td:nth-child(2) > table.torrentname > tbody > tr > td.embedded > span:not(.optiontag)",
              "remove": "img,a,b"
            },
            {
              "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span:not(.optiontag)",
              "remove": "img,a,b"
            }
          ],
          "filters": [
            {
              "name": "replace",
              "args": [
                "[优惠剩余时间：]",
                ""
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "any": [
            {
              "selector": "td:nth-child(2) > table.torrentname > tbody > tr > td.embedded > span.optiontag"
            },
            {
              "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span.optiontag"
            }
          ]
        }
      }
    },
    "sign_in_image_code": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/image_code_ajax.php",
      "disabled_extends": {
        "params": true
      },
      "form_data": {
        "action": [
          "new"
        ]
      },
      "headers": {
        "Referer": "https://hdsky.me/index.php",
        "X-Requested-With": "XMLHttpRequest"
      },
      "fields": {
        "success": {},
        "code": {}
      }
    },
    "sign_in_image": {
      "parser": "None",
      "path": "/image.php",
      "disabled_extends": {
        "params": true
      },
      "params": {
        "action": [
          "regimage"
        ],
        "imagehash": [
          "{image_hash}"
        ]
      },
      "headers": {
        "Referer": "https://hdsky.me/index.php"
      }
    },
    "sign_in_showup": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/showup.php",
      "disabled_extends": {
        "params": true
      },
      "form_data": {
        "action": [
          "showup"
        ],
        "imagehash": [
          "{image_hash}"
        ],
        "imagestring": [
          "{image_string}"
        ]
      },
      "headers": {
        "Referer": "https://hdsky.me/index.php",
        "X-Requested-With": "XMLHttpRequest"
      },
      "fields": {
        "success": {},
        "message": {}
      }
    }
  }
}
