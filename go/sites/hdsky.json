  "requests": {
    "search": {
      "fields": {
        "category": {
          "selector": "a[href*='?cat=']",
          "attribute": "href",
          "filters": [
            {
              "name": "replace",
              "args": [
                "?",
                ""
              ]
            },
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "download": {
          "selector": "form[action*='/download.php?id=']",
          "attribute": "action"
        },
        "description": {
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span:not(.optiontag)",
          "remove": "img,a,b",
          "filters": [
            {
              "name": "replace",
              "args": [
                "[优惠剩余时间：]",
                ""
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span.optiontag"
        }
      }
    }
  }
