{
  "id": "hdsky",
  "name": "天空",
  "domain": "https://hdsky.me/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "sign_in_required": true,
  "price": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      },
      {
        "id": 410,
        "cat": "Movies",
        "desc": "iPad/iPad影视"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/剧集(分集)"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows/综艺"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      },
      {
        "id": 411,
        "cat": "TV",
        "desc": "TV Series/剧集(合集)"
      },
      {
        "id": 412,
        "cat": "TV",
        "desc": "TV Series/海外剧集(分集)"
      },
      {
        "id": 413,
        "cat": "TV",
        "desc": "TV Series/海外剧集(合集)"
      },
      {
        "id": 414,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(分集)"
      },
      {
        "id": 415,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(合集)"
      }
    ]
  },
  "requests": {
    "search": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "torrents.php",
      "params": {
        "search": [
          "{keyword}"
        ]
      },
      "list": {
        "selector": "table.torrents tr:has(table.torrentname)"
      },
      "fields": {
        "id": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        },
        "title": {
          "any": [
            {
              "selector": "a[href*=\"details.php?id=\"]"
            },
            {
              "selector": "a[title][href*=\"details.php?id=\"]",
              "attribute": "title"
            }
          ]
        },
        "category": {
          "selector": "a[href*=\"?cat=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "replace",
              "args": [
                "?",
                ""
              ]
            },
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "details": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href"
        },
        "download": {
          "selector": "form[action*=\"/download.php?id=\"]",
          "attribute": "action"
        },
        "imdbid": {
          "selector": "div.imdb_100 > a",
          "attribute": "href"
        },
        "date_elapsed": {
          "selector": "td:nth-child(4) > span",
          "optional": true
        },
        "date_added": {
          "selector": "td:nth-child(4) > span",
          "attribute": "title",
          "optional": true
        },
        "size": {
          "selector": "td.rowfollow:nth-child(5)"
        },
        "seeders": {
          "selector": "td:nth-child(6)"
        },
        "leechers": {
          "selector": "td:nth-child(7)"
        },
        "grabs": {
          "selector": "td:nth-child(8)"
        },
        "downloadvolumefactor": {
          "case": {
            "img.pro_free": 0,
            "img.pro_free2up": 0,
            "img.pro_50pctdown": 0.5,
            "img.pro_50pctdown2up": 0.5,
            "img.pro_30pctdown": 0.3,
            "*": 1
          }
        },
        "uploadvolumefactor": {
          "case": {
            "img.pro_50pctdown2up": 2,
            "img.pro_free2up": 2,
            "img.pro_2up": 2,
            "*": 1
          }
        },
        "description": {
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span:not(.optiontag)",
          "remove": "img,a,b",
          "filters": [
            {
              "name": "replace",
              "args": [
                "[优惠剩余时间：]",
                ""
              ]
            }
          ]
        },
        "labels": {
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span.optiontag"
        }
      }
    }
  }
}
