{
  "id": "hdsky",
  "name": "天空",
  "domain": "https://hdsky.me",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      },
      {
        "id": 410,
        "cat": "Movies",
        "desc": "iPad/iPad影视"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/剧集(分集)"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows/综艺"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      },
      {
        "id": 411,
        "cat": "TV",
        "desc": "TV Series/剧集(合集)"
      },
      {
        "id": 412,
        "cat": "TV",
        "desc": "TV Series/海外剧集(分集)"
      },
      {
        "id": 413,
        "cat": "TV",
        "desc": "TV Series/海外剧集(合集)"
      },
      {
        "id": 414,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(分集)"
      },
      {
        "id": 415,
        "cat": "TV",
        "desc": "TV Shows/海外综艺(合集)"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "category": {
          "selector": "a[href*='?cat=']",
          "attribute": "href",
          "filters": [
            {
              "name": "replace",
              "args": [
                "?",
                ""
              ]
            },
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "download": {
          "selector": "form[action*='/download.php?id=']",
          "attribute": "action"
        },
        "imdbid": {
          "selector": "div.imdb_100 > a",
          "attribute": "href"
        },
        "description": {
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span:not(.optiontag)",
          "remove": "img,a,b",
          "filters": [
            {
              "name": "replace",
              "args": [
                "[优惠剩余时间：]",
                ""
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "td:nth-child(2) > table.torrentname > tr > td.embedded > span.optiontag"
        }
      }
    }
  }
}
