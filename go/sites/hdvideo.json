{
  "id": "hdvideo",
  "name": "HDVIDEO",
  "domain": "https://hdvideo.one",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "inactive_day": 7,
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV/Series",
        "desc": "电视剧"
      },
      {
        "id": 403,
        "cat": "TV/Shows",
        "desc": "综艺"
      },
      {
        "id": 404,
        "cat": "TV/Documentaries",
        "desc": "纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Animations",
        "desc": "动漫"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "1",
                "0"
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "td.embedded > span[title]"
        },
        "description": {
          "selector": "td.embedded:nth-child(1)",
          "remove": "span.label,a,img,b,br",
          "contents": -1
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
}
