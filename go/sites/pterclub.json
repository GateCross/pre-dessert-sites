{
  "id": "pterclub",
  "name": "猫站",
  "domain": "https://pterclub.com",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true,
    "has_cover": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影 (Movie)"
      }
    ],
    "tv": [
      {
        "id": 404,
        "cat": "TV",
        "desc": "电视剧 (TV Play)"
      },
      {
        "id": 403,
        "cat": "TV/Anime",
        "desc": "动漫 (Anime)"
      },
      {
        "id": 405,
        "cat": "TV",
        "desc": "综艺 (TV Show)"
      },
      {
        "id": 402,
        "cat": "TV/Documentary",
        "desc": "纪录片 (Documentary)"
      }
    ]
  },
  "requests": {
    "user_basic_info": {
      "fields": {
        "bonus": {
          "selector": "//*[@id='info_block']/tbody/tr/td/table/tbody/tr/td[1]/span/text()[9]",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\s*([\\d,]+\\.\\d+)",
                1
              ]
            }
          ]
        }
      }
    },
    "user_details": {
      "fields": {
        "level": {
          "selector": "//img[contains(@src, 'pic/user_class')]/@alt"
        },
        "level_icon": {
          "selector": "//img[contains(@src, 'pic/user_class')]/@src"
        }
      }
    },
    "search": {
      "fields": {
        "title": {
          "selector": "td:nth-child(2) > div > div:nth-child(1) > a > b"
        },
        "description": {
          "selector": "td:nth-child(2) > div > div:nth-child(2) > span"
        },
        "labels": {
          "array": true,
          "selector": "td:nth-child(2) > div > div:nth-child(2) > a.torrents-tag"
        },
        "cover": {
          "selector": "td:nth-child(1) > img",
          "attribute": "data-orig"
        }
      }
    },
    "details": {
      "fields": {
        "peer_count": {
          "selector": "(//td[@align='left' and @class='rowfollow' and @valign='top']/b[1])[3]",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        }
      }
    },
    "seeding_statistics": {
      "path": "/getusertorrentlist.php?do_ajax=1&userid={userId}&type=seeding",
      "headers": {
        "accept": "*/*",
        "accept-language": "zh-CN,zh;q=0.9",
        "cache-control": "no-cache",
        "pragma": "no-cache",
        "priority": "u=1, i",
        "referer": "{domain}/userdetails.php?id={userId}",
        "sec-fetch-dest": "empty",
        "sec-fetch-mode": "cors",
        "sec-fetch-site": "same-origin",
        "x-requested-with": "XMLHttpRequest"
      }
    },
    "sign_in": {
      "path": "/attendance-ajax.php"
    }
  }
}
