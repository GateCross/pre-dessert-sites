{
  "id": "yemapt",
  "name": "野马",
  "domain": "https://www.yemapt.org/",
  "api": "https://www.yemapt.org/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "reuse_schema": "NexusPHP",
  "required": {
    "cookie": true
  },
  "price": {
    "has_free": true
  },
  "requests":{
    "search": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/fetchOpenTorrentList",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {"keyword": "{keyword}","pageParam":{"current": "{page}","pageSize":20,"pageSizeOptions":["10","20","40"],"size":"small"},"sorter":{}},
      "list": {
        "selector": "data"
      },
      "fields": {
        "id": {
          "selector": "id"
        },
        "title": {
          "selector": "showName"
        },
        "details": {
          "selector": "id",
          "filters": [
            {
              "name": "append_left",
              "args": "/#/torrent/detail/"
            }
          ]
        },
        "size": {
          "selector": "fileSize"
        },
        "seeders": {
          "selector": "seedNum"
        },
        "leechers": {
          "selector": "leechNum"
        },
        "date_added": {
          "selector": "listingTime",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "downloadvolumefactor": {
          "selector": "downloadPromotion",
          "filters": [
            {
              "name": "case",
              "args": {
                "free": 0,
                "half": 0.5,
                "percent_70": 0.3,
                "normal": 1
              }
            }
          ]
        },
        "uploadvolumefactor": {
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "description": {
          "selector": "shortDesc"
        },
        "labels": {
          "selector": "tagList",
          "array": true,
          "filters": [
            {
              "name": "case",
              "args": {
                "0": "",
                "1": "",
                "2": "",
                "4": "",
                "5": "国语",
                "6": "中字",
                "7": "粤语",
                "8": "英字",
                "10": "杜比视界",
                "11": "分集",
                "12": "完结"
              }
            }
          ]
        }
      }
    },
    "user_basic_info": {
      "parser": "JsonPath",
      "method": "GET",
      "path": "/api/consumer/fetchSelfDetail",
      "headers": {
        "Content-Type": "application/json"
      },
      "fields": {
        "created_date": {
          "selector": "data.registerTime",
          "filters": [
            {
              "name": "timestamp",
              "args": [
                "2006-01-02 15:04:05",
                "2024-05-13T01:51:53.000+00:00"
              ]
            }
          ]
        },
        "username": {
          "selector": "data.name"
        },
        "uploaded": {
          "selector": "data.promotionUploadSize"
        },
        "downloaded": {
          "selector": "data.promotionDownloadSize"
        },
        "bonus": {
          "selector": "data.bonus"
        }
      }
    },
    "seeding_statistics": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/fetchUserTorrentList",
      "headers": {
        "Content-Type": "application/json"
      },
      "list": {
        "selector": "data"
      },
      "fields": {
        "size": {
          "selector": "fileSize",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    }
  }
}
