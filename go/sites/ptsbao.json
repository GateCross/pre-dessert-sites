{
  "id": "ptsbao",
  "name": "烧包乐园",
  "domain": "https://ptsbao.club",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": false
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies(电影)"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series(电视剧)"
      },
      {
        "id": 403,
        "cat": "TV/Other",
        "desc": "TV Shows(电视节目)"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations(动漫)"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries(记录片)"
      }
    ]
  },
  "requests":{
    "search": {
      "fields": {
        "hr_days":{
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "labels": {
          "selector": "td.embedded > span.tag",
          "array": true
        },
        "description": {
          "selector": "td.embedded",
          "remove": "span.label,a,img,b,br",
          "contents": -1
        },
        "size": {
          "selector": "td:nth-child(6)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "grabs": {
          "selector": "td:nth-child(9)"
        },
        "seeders": {
          "selector": "td:nth-child(7)"
        },
        "date_added": {
          "selector": "td:nth-child(5):not(.embedded) > span",
          "attribute": "title",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        }
      }
    }
  },
  "my_hr": {
    "path": "/hr.php?hr=1"
  }
}
