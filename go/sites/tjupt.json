{
  "id": "tjupt",
  "name": "北洋园",
  "domain": "https://tjupt.org/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies电影"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series剧集"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows综艺"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations动漫"
      }
    ]
  },
  "price": {
    "FREE": [
      "//h1[@id='top']/b/font[@class='free']"
    ],
    "2XFREE": [
      "//font[@class='twoup'][text()='2X']"
    ],
    "HR": [
      "//font[@color='red'][text()='Hit&Run']"
    ],
    "PEER_COUNT": [
      "//div[@id='peercount']/b[1]"
    ]
  },
  "requests": {
    "search": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "torrents.php",
      "list": {
        "selector": "table.torrents tr:has(table.torrentname)"
      },
      "params": {
        "search": [
          "{keyword}"
        ]
      },
      "fields": {
        "id": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        },
        "category": {
          "selector": "a[href*=\"cat=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "title": {
          "any": [
            {
              "selector": "a[href*=\"details.php?id=\"]"
            },
            {
              "selector": "a[title][href*=\"details.php?id=\"]",
              "attribute": "title"
            }
          ]
        },
        "details": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href"
        },
        "download": {
          "selector": "a[href*=\"download.php?id=\"]",
          "attribute": "href"
        },
        "imdbid": {
          "selector": "a[href*=\"imdb.com/title/tt\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "tt\\d+",
                1
              ]
            }
          ]
        },
        "date_elapsed": {
          "selector": "td.rowfollow:nth-child(4)",
          "remove": "br",
          "optional": true
        },
        "date_added": {
          "selector": "td.rowfollow:nth-child(4)",
          "remove": "br",
          "optional": true
        },
        "size": {
          "selector": "td:nth-child(5)"
        },
        "seeders": {
          "selector": "td:nth-child(6)"
        },
        "leechers": {
          "selector": "td:nth-child(7)"
        },
        "grabs": {
          "selector": "td:nth-child(8)"
        },
        "downloadvolumefactor": {
          "case": {
            "font.free": 0,
            "font.twoupfree": 0,
            "font.halfdown": 0.5,
            "font.twouphalfdown": 0.5,
            "font.thirtypercent": 0.3,
            "*": 1
          }
        },
        "uploadvolumefactor": {
          "case": {
            "font.twouphalfdown": 2,
            "font.twoupfree": 2,
            "font.twoup": 2,
            "*": 1
          }
        },
        "description": {
          "selector": "table.torrentname > tr:nth-child(1) > td:nth-child(2)",
          "remove": "a, img, div",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                -1
              ]
            }
          ]
        },
        "labels": {
          "selector": "table.torrentname > tr:nth-child(2) > td:nth-child(2) > div#tag > span > font.tag"
        },
        "minimumratio": {
          "text": 0.8
        }
      }
    },
    "seeding": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "getusertorrentlistajax.php?userid={userId}&type=seeding",
      "fields": {
        "size": {
          "selector": "*",
          "selection": "html",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "总大小(.*)<br/>",
                1
              ]
            }
          ]
        },
        "count": {
          "selector": "b"
        }
      }
    },
    "my_hr": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "hnr_details.php?show=ongoing",
      "list": {
        "selector": "table.coltable tr:has(td.rowfollow)"
      },
      "fields": {
        "id": {
          "selector": "td:nth-child(1)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "考核ID：(.*)",
                1
              ]
            }
          ]
        },
        "name": {
          "selector": "td:nth-child(2) b"
        },
        "uploaded": {
          "selector": "td:nth-child(3)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "(.*)\\(.*\\)",
                1
              ]
            }
          ]
        },
        "downloaded": {
          "selector": "td:nth-child(4)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "(.*)\\(.*\\)",
                1
              ]
            }
          ]
        },
        "share_ratio": {
          "selector": "td:nth-child(3)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\(分享率：(.*)\\)",
                1
              ]
            }
          ]
        },
        "need_seed_time": {
          "selector": "td:nth-child(7)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "剩余：(.*)",
                1
              ]
            }
          ]
        },
        "download_time": {
          "selector": "td:nth-child(1)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "(\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2})",
                1
              ]
            }
          ]
        },
        "remaining_time": {
          "selector": "td:nth-child(8)",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "考核期限：(.*)",
                1
              ]
            }
          ]
        }
      }
    }
  }
}
