  "requests": {
    "user_basic_info": {
      "fields": {
        "bonus": {
          "selector": "//*[@id='info_block']/tbody/tr/td/table/tbody/tr/td[1]/span/text()[7]",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\s*([\\d,]+\\.\\d+)",
                1
              ]
            }
          ]
        }
      }
    },
    "search": {
      "fields": {
        "category": {
          "selector": "a[href*='?cat=']",
          "attribute": "href",
          "filters": [
            {
              "name": "replace",
              "args": [
                "?",
                ""
              ]
            },
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "description": {
          "selector": "td.embedded > span[style]",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                -1
              ]
            }
          ]
        },
        "labels": {
          "selector": "td:nth-child(2) > table > tbody > tr > td.embedded > span.tags",
          "array": true
        },
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "14",
                "0"
              ]
            }
          ]
        }
      }
    },
    "seeding_statistics": {
      "parser": "XPath",
      "disabled_extends": {
        "headers": true,
        "list": true
      },
      "fields": {
        "size": {
          "selector": "*",
          "selection": "html",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "Total:\\s(\\d+\\.\\d+ \\wB)",
                1
              ]
            },
            {
              "name": "byte_size"
            }
          ]
        },
        "count": {
          "selector": "//b[1]/text()"
        }
      }
    }
  }
