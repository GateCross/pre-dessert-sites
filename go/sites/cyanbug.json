{
  "id": "cyanbug",
  "name": "大青虫",
  "domain": "https://cyanbug.net/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "sign_in_required": true,
  "price": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": false
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies(电影)",
        "default": true
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series(电视剧)",
        "default": true
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows(综艺)",
        "default": true
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries(纪录片)",
        "default": true
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations(动画)",
        "default": true
      }
    ]
  },
  "requests": {
    "search": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "torrents.php",
      "list": {
        "selector": "table.torrents tr:has(table.torrentname)"
      },
      "params": {
        "search": [
          "{keyword}"
        ]
      },
      "fields": {
        "id": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        },
        "category": {
          "selector": "a[href*=\"?cat=\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "querystring",
              "args": "cat"
            }
          ]
        },
        "title": {
          "any": [
            {
              "selector": "a[href*=\"details.php?id=\"]"
            },
            {
              "selector": "a[title][href*=\"details.php?id=\"]",
              "attribute": "title"
            }
          ]
        },
        "details": {
          "selector": "a[href*=\"details.php?id=\"]",
          "attribute": "href"
        },
        "poster": {
          "selector": "img[data-orig]",
          "attribute": "data-orig"
        },
        "download": {
          "selector": "a[href*=\"download.php?id=\"]",
          "attribute": "href"
        },
        "size": {
          "selector": "td.rowfollow:nth-child(5)"
        },
        "grabs": {
          "selector": "td.rowfollow:nth-child(8)"
        },
        "seeders": {
          "selector": "td.rowfollow:nth-child(6)"
        },
        "leechers": {
          "selector": "td.rowfollow:nth-child(7)"
        },
        "date_elapsed": {
          "selector": "td.rowfollow:nth-child(4) > span",
          "optional": true
        },
        "date_added": {
          "selector": "td.rowfollow:nth-child(4) > span",
          "attribute": "title",
          "optional": true
        },
        "downloadvolumefactor": {
          "case": {
            "img.pro_free": 0,
            "img.pro_free2up": 0,
            "img.pro_50pctdown": 0.5,
            "img.pro_50pctdown2up": 0.5,
            "img.pro_30pctdown": 0.3,
            "*": 1
          }
        },
        "uploadvolumefactor": {
          "case": {
            "img.pro_50pctdown2up": 2,
            "img.pro_free2up": 2,
            "img.pro_2up": 2,
            "*": 1
          }
        },
        "tags": {
          "selector": "div > a.torrents-tag"
        },
        "subject": {
          "selector": "td.embedded:nth-child(2) > div > div:nth-child(2) > span",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                -1
              ]
            }
          ]
        },
        "description": {
          "selector": "td:nth-child(2) > table > tbody > tr > td:nth-child(2)",
          "remove": "span,a,img,font,b",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                -1
              ]
            }
          ]
        },
        "labels": {
          "selector": "td:nth-child(2) > span[title=\"\"]"
        }
      }
    },
    "seeding_statistics": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "getusertorrentlistajax.php?userid={userId}&type=seeding",
      "fields": {
        "size": {
          "selector": "div > div:nth-child(1)",
          "remove": "b",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "总大小：(.*)",
                1
              ]
            },
            {
              "name": "byte_size"
            }
          ]
        },
        "count": {
          "selector": "div > div:nth-child(1) > b"
        }
      }
    }
  }
}
