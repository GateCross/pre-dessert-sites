{
  "id": "cyanbug",
  "name": "大青虫",
  "domain": "https://cyanbug.net",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies(电影)",
        "default": true
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series(电视剧)",
        "default": true
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows(综艺)",
        "default": true
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries(纪录片)",
        "default": true
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations(动画)",
        "default": true
      }
    ]
  },
  "requests": {
    "search": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "/torrents.php",
      "params": {
        "search": [
          "{keyword}"
        ]
      }
    },
    "seeding_statistics": {
      "parser": "CssSelector",
      "disabled_extends": {
        "headers": true,
        "list": true
      },
      "fields": {
        "size": {
          "selector": "div > div:nth-child(1)",
          "remove": "b",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "总大小：(.*)",
                1
              ]
            },
            {
              "name": "byte_size"
            }
          ]
        },
        "count": {
          "selector": "div > div:nth-child(1) > b"
        }
      }
    }
  }
}
