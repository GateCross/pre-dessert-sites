{
  "id": "ssd",
  "name": "春天",
  "domain": "https://springsunday.net",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true
  },
  "price": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 501,
        "cat": "Movies",
        "desc": "Movies(电影)"
      }
    ],
    "tv": [
      {
        "id": 502,
        "cat": "TV",
        "desc": "TV Series(剧集)"
      },
      {
        "id": 503,
        "cat": "TV/Documentary",
        "desc": "Docs(纪录)"
      },
      {
        "id": 504,
        "cat": "TV/Anime",
        "desc": "Animations(动画)"
      },
      {
        "id": 505,
        "cat": "TV",
        "desc": "TV Shows(综艺)"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "imdbid": {
          "selector": "a[href*=\"imdb.com/title/tt\"]",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "tt\\d+",
                0
              ]
            }
          ]
        },
        "date_elapsed": {
          "selector": "td:nth-last-child(6) > span"
        },
        "date_added": {
          "selector": "td:nth-last-child(6) > span",
          "attribute": "title",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "size": {
          "selector": "td:nth-last-child(5)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "seeders": {
          "selector": "td:nth-last-child(4)"
        },
        "leechers": {
          "selector": "td:nth-last-child(3)"
        },
        "grabs": {
          "selector": "td:nth-last-child(2)"
        },
        "downloadvolumefactor": {
          "any": [
            {
              "selector": "span.torrent-pro-icon,span.torrent-pro-free,span.torrent-pro-twoupfree",
              "attribute": "class",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0,
                    ""
                  ]
                }
              ]
            },
            {
              "selector": "span.torrent-pro-icon,span.torrent-pro-halfdown,span.torrent-pro-twouphalfdown",
              "attribute": "class",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0.5,
                    ""
                  ]
                }
              ]
            },
            {
              "selector": "span.torrent-pro-icon,span.torrent-pro-thirtypercent",
              "attribute": "class",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0.3,
                    ""
                  ]
                }
              ]
            }
          ]
        },
        "uploadvolumefactor": {
          "any": [
            {
              "selector": "span.torrent-pro-icon,span.torrent-pro-twouphalfdown,span.torrent-pro-twoupfree,span.torrent-pro-twoup",
              "attribute": "class",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    2,
                    1
                  ]
                }
              ]
            }
          ]
        },
        "description": {
          "selector": "table.torrentname > tr > td > div.torrent-smalldescr > span",
          "remove": "a, b, img, span",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+-\\d+-\\d+ \\d+:\\d+:\\d+",
                0
              ]
            },
            {
              "name": "split",
              "args": [
                "\n",
                0
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "table.torrentname > tr > td > div.torrent-smalldescr > a > span"
        }
      }
    },
    "latest_notice": {
      "fields": {
        "title": {
          "selector": "//*[@id='outer']/div[2]/div[1]/div/div[1]/a/span/b"
        },
        "date": {
          "selector": "/html/body/main/div[2]/div[2]/div[1]/div/div[1]/a/span",
          "remove": "b",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "(\\d{4}\\.\\d{2}\\.\\d{2})",
                1
              ]
            },
            {
              "name": "timestamp"
            }
          ]
        },
        "content": {
          "selector": "//*[@id='outer']/div[2]/div[1]/div/div[1]/div"
        }
      }
    }
  }
}
