{
  "id": "ptvicomo",
  "name": "象站",
  "domain": "https://ptvicomo.net",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "inactive_day": 7,
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      }
    ],
    "tv": [
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      },
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/连续剧"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows/综艺"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "size": {
          "selector": "td:nth-child(4)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "grabs": {
          "selector": "td:nth-child(7)"
        },
        "seeders": {
          "selector": "td:nth-child(5)"
        },
        "leechers": {
          "selector": "td:nth-child(6)"
        },
        "date_elapsed": {
          "selector": "td:nth-child(3)  > b > span"
        },
        "date_added": {
          "selector": "td:nth-child(3)  > b > span",
          "attribute": "title",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "7",
                "0"
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "td.embedded > span[title]"
        },
        "description": {
          "selector": "td.embedded:nth-child(1)",
          "remove": "span.label,a,img,b,br",
          "contents": -1
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
}
