{
  "id": "cyanbug",
  "name": "大青虫",
  "domain": "https://cyanbug.net/",
  "encoding": "UTF-8",
  "public": false,
  "request_basics": {
    "fields": {},
    "search": {
      "params": {
        "search": "{keyword}"
      },
      "batch": {
        "delimiter": " ",
        "space_replace": "_"
      }
    }
  },
  "request_definitions": {
    "search": {
      "analysis": "CssSelector",
      "method": "GET",
      "path": "torrents.php",
      "fields": {
        "list": {
          "type": "table",
          "css_selector": "table.torrents > tbody > tr:has(table.torrentname)",
          "columns": {
            "id": {
              "css_selector": "a[href*=\"details.php?id=\"]",
              "attribute": "href",
              "filters": [
                {
                  "name": "re_search",
                  "args": [
                    "\\d+",
                    0
                  ]
                }
              ]
            },
            "category": {
              "css_selector": "a[href*=\"?cat=\"]",
              "attribute": "href",
              "filters": [
                {
                  "name": "querystring",
                  "args": "cat"
                }
              ]
            },
            "title_default": {
              "css_selector": "a[href*=\"details.php?id=\"]"
            },
            "title_optional": {
              "optional": true,
              "css_selector": "a[title][href*=\"details.php?id=\"]",
              "attribute": "title"
            },
            "details": {
              "css_selector": "a[href*=\"details.php?id=\"]",
              "attribute": "href"
            },
            "poster": {
              "css_selector": "img[data-orig]",
              "attribute": "data-orig"
            },
            "download": {
              "css_selector": "a[href*=\"download.php?id=\"]",
              "attribute": "href"
            },
            "size": {
              "css_selector": "td.rowfollow:nth-child(5)"
            },
            "grabs": {
              "css_selector": "td.rowfollow:nth-child(8)"
            },
            "seeders": {
              "css_selector": "td.rowfollow:nth-child(6)"
            },
            "leechers": {
              "css_selector": "td.rowfollow:nth-child(7)"
            },
            "date_elapsed": {
              "css_selector": "td.rowfollow:nth-child(4) > span",
              "optional": true
            },
            "date_added": {
              "css_selector": "td.rowfollow:nth-child(4) > span",
              "attribute": "title",
              "optional": true
            },
            "downloadvolumefactor": {
              "case": {
                "img.pro_free": 0,
                "img.pro_free2up": 0,
                "img.pro_50pctdown": 0.5,
                "img.pro_50pctdown2up": 0.5,
                "img.pro_30pctdown": 0.3,
                "*": 1
              }
            },
            "uploadvolumefactor": {
              "case": {
                "img.pro_50pctdown2up": 2,
                "img.pro_free2up": 2,
                "img.pro_2up": 2,
                "*": 1
              }
            },
            "tags": {
              "css_selector": "div > a.torrents-tag"
            },
            "subject": {
              "css_selector": "td.embedded:nth-child(2) > div > div:nth-child(2) > span",
              "contents": -1
            },
            "description": {
              "css_selector": "td:nth-child(2) > table > tbody > tr > td:nth-child(2)",
              "remove": "span,a,img,font,b",
              "contents": -1
            },
            "labels": {
              "css_selector": "td:nth-child(2) > span[title=\"\"]"
            }
          }
        }
      }
    },
    "seeding": {
      "analysis": "CssSelector",
      "method": "GET",
      "path": "getusertorrentlistajax.php?userid={userId}&type=seeding",
      "fields": {
        "size": {
          "css_selector": "div > div:nth-child(1)",
          "remove": "b",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "总大小：(.*)",
                1
              ]
            }
          ]
        },
        "count": {
          "css_selector": "div > div:nth-child(1) > b"
        }
      }
    }
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies(电影)",
        "default": true
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series(电视剧)",
        "default": true
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows(综艺)",
        "default": true
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries(纪录片)",
        "default": true
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations(动画)",
        "default": true
      }
    ]
  }
}
