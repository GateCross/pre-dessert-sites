{
  "id": "audiences",
  "name": "观众",
  "domain": "https://audiences.me/",
  "encoding": "UTF-8",
  "public": false,
  "schema": "NexusAudiences",
  "request_basics": {
    "fields": {},
    "search": {
      "params": {
        "search": "{keyword}"
      },
      "batch": {
        "delimiter": " ",
        "space_replace": "_"
      }
    }
  },
  "request_definitions": {
    "search": {
      "analysis": "CssSelector",
      "method": "GET",
      "path": "torrents.php",
      "fields": {
        "list": {
          "type": "table",
          "css_selector": "table.torrents > tbody > tr:has(table.torrentname)",
          "columns": {
            "id": {
              "css_selector": "a[href*=\"details.php?id=\"]",
              "attribute": "href",
              "filters": [
                {
                  "name": "re_search",
                  "args": [
                    "\\d+",
                    0
                  ]
                }
              ]
            },
            "title_default": {
              "css_selector": "a[href*=\"details.php?id=\"]"
            },
            "title_optional": {
              "optional": true,
              "css_selector": "a[title][href*=\"details.php?id=\"]",
              "attribute": "title"
            },
            "category": {
              "css_selector": "a[href*=\"?cat=\"]",
              "attribute": "href",
              "filters": [
                {
                  "name": "replace",
                  "args": [
                    "?",
                    ""
                  ]
                },
                {
                  "name": "querystring",
                  "args": "cat"
                }
              ]
            },
            "details": {
              "css_selector": "a[href*=\"details.php?id=\"]",
              "attribute": "href"
            },
            "download": {
              "css_selector": "a[href*=\"download.php?id=\"]",
              "attribute": "href"
            },
            "imdbid": {
              "css_selector": "div.imdb_100 > a",
              "attribute": "href",
              "filters": [
                {
                  "name": "re_search",
                  "args": [
                    "tt\\d+",
                    0
                  ]
                }
              ]
            },
            "date_elapsed": {
              "css_selector": "td:nth-child(4) > span",
              "optional": true
            },
            "date_added": {
              "css_selector": "td:nth-child(4) > span",
              "attribute": "title",
              "optional": true
            },
            "size": {
              "css_selector": "td:nth-child(5)"
            },
            "seeders": {
              "css_selector": "td:nth-child(6)"
            },
            "leechers": {
              "css_selector": "td:nth-child(7)"
            },
            "grabs": {
              "css_selector": "td:nth-child(8)"
            },
            "downloadvolumefactor": {
              "case": {
                "img.pro_free": 0,
                "img.pro_free2up": 0,
                "img.pro_50pctdown": 0.5,
                "img.pro_50pctdown2up": 0.5,
                "img.pro_30pctdown": 0.3,
                "*": 1
              }
            },
            "uploadvolumefactor": {
              "case": {
                "img.pro_50pctdown2up": 2,
                "img.pro_free2up": 2,
                "img.pro_2up": 2,
                "*": 1
              }
            },
            "description": {
              "css_selector": "td.embedded > span[style]"
            },
            "labels": {
              "css_selector": "td.embedded > span.tags"
            },
            "hr_days": {
              "default_value": "2",
              "css_selector": "img.hitandrun",
              "attribute": "title",
              "filters": [
                {
                  "name": "re_search",
                  "args": [
                    "\\d*",
                    0
                  ]
                }
              ]
            }
          }
        }
      }
    },
    "seeding": {
      "analysis": "CssSelector",
      "method": "GET",
      "path": "getusertorrentlistajax.php?userid={userId}&type=seeding",
      "headers": {
        "Referer": "https://audiences.me/userdetails.php?id={userId}"
      },
      "fields": {
        "count": {
          "css_selector": "table.table.table-bordered tr:has(td:contains(Total)) td:nth-child(2)"
        },
        "size": {
          "css_selector": "table.table.table-bordered tr:has(td:contains(Total)) td:nth-child(3)"
        }
      }
    }
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "电影/Movies"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "剧集/TV-Series"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "综艺/TV-Show"
      },
      {
        "id": 406,
        "cat": "TV/Documentary",
        "desc": "纪录片/Documentary"
      }
    ]
  }
}
